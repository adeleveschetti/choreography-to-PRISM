s4 : [0..1];

Station4_STATE: [0..10] init 0;

[PKSIF] (s4=0) -> 1: (s4'=0);
[IWCGE] (s4=0) -> lambda: (s4'=1);
[RBMMX] (s4=1) -> 1: (s4'=0);

endmodule 

