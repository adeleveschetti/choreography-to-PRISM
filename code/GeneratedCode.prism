s5 : [0..1];

Station5_STATE: [0..10] init 0;

[KWDMX] (s5=0) -> 1: (s5'=0);
[UBEGV] (s5=0) -> lambda: (s5'=1);
[SLOGT] (s5=1) -> 1: (s5'=0);

endmodule 

