Station3_STATE: [0..10] init 0;

[DEHHY] (s3=0) -> 1: (s3'=0);
[GNLBM] (s3=0) -> lambda: (s3'=1);
[RCDEJ] (s3=1) -> 1: (s3'=0);

endmodule 

