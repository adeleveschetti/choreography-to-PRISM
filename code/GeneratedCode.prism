s2 : [0..1];

Station2_STATE: [0..10] init 0;

[HHRUR] (s2=0) -> 1: (s2'=0);
[GWGOU] (s2=0) -> lambda: (s2'=1);
[UPQDF] (s2=1) -> 1: (s2'=0);

endmodule 

