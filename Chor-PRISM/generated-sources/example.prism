[RAROS] (Network=0)  -> 1 : (set1'=addBlockSet(set1,b1))&(set2'=addBlockSet(set2,b2))&(set3'=addBlockSet(set3,b3))&(Network'=0); 

[WEBJU] (Network=0)  -> 1 : (set4' = removeBlock(set4,b4))&(Network'=0); 
endmodule


