module Network
Network : [0..1] init 0;
set1 : list []; 
set2 : list []; 
set3 : list []; 
set4 : list []; 


[YWLHU] (Network=0)  -> 1 : (set2'=addBlockSet(set2,b2))&(set3'=addBlockSet(set3,b3))&(set4'=addBlockSet(set4,b4))&(Network'=0); 

[APPBC] (Network=0)  -> 1 : (set1' = removeBlock(set1,b1))&(Network'=0); 

[SJSXH] (Network=0)  -> 1 : (set1'=addBlockSet(set1,b1))&(set3'=addBlockSet(set3,b3))&(set4'=addBlockSet(set4,b4))&(Network'=0); 

[PKMMA] (Network=0)  -> 1 : (set2' = removeBlock(set2,b2))&(Network'=0); 

[WZOIU] (Network=0)  -> 1 : (set1'=addBlockSet(set1,b1))&(set2'=addBlockSet(set2,b2))&(set4'=addBlockSet(set4,b4))&(Network'=0); 

[YWHBX] (Network=0)  -> 1 : (set3' = removeBlock(set3,b3))&(Network'=0); 

[EKQMZ] (Network=0)  -> 1 : (set1'=addBlockSet(set1,b1))&(set2'=addBlockSet(set2,b2))&(set3'=addBlockSet(set3,b3))&(Network'=0); 

[HIWQW] (Network=0)  -> 1 : (set4' = removeBlock(set4,b4))&(Network'=0); 
endmodule


