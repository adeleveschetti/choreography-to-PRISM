module Network
set1 : list [];
set2 : list [];
set3 : list [];
set4 : list [];
Network : [0..10] init 0;


[MFDJJ] (Network=0)  -> 1 : (set2'=addBlockSet(set2,b2))&(set3'=addBlockSet(set3,b3))&(set4'=addBlockSet(set4,b4))&(Network'=0); 

[WVHER] (Network=0)  -> 1 : (set1' = removeBlock(set1,b1))&(Network'=0); 

[AODVN] (Network=0)  -> 1 : (set1'=addBlockSet(set1,b1))&(set3'=addBlockSet(set3,b3))&(set4'=addBlockSet(set4,b4))&(Network'=0); 

