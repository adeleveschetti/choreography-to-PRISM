ctmc
const k;
const T=k*1000;
const double r = 1;
const double mR = 1/600;
const double lR = 1-mR;
const double hR1 = 0.25;
const double hR2 = 0.25;
const double hR3 = 0.25;
const double hR4 = 0.25;
const double rB = 1/12.6;
const int N = 100;

module Miner1
Miner1 : [0..7] init 0;
b1 : block {m1,0;genesis,0} ; 
B1 : blockchain [{genesis,0;genesis,0}]; 
c1 : [0..N] init 0; 
setMiner1 : list []; 


[SJWPS] (Miner1=0)  -> hR1 : (b1'=createB(b1,B1,c1))&(c1'=c1+1)&(Miner1'=1); 
[GYNCQ] (Miner1=0)  -> hR1 :  (Miner1'=2); 

[YWLHU] (Miner1=1)  -> rB : (B1'=addBlock(B1,b1))&(Miner1'=0); 

[] (Miner1=2)&!isEmpty(set1) -> r : (b1'=extractBlock(set1))&(Miner1'=4); 

[APPBC] (Miner1=4)  -> 1 : (setMiner1' = addBlockSet(setMiner1 , b1))&(Miner1'=0); 

[] (Miner1=2)&!(!isEmpty(set1)) -> 1 : (Miner1'=5); 

[] (Miner1=5)&canBeInserted(B1,b1) -> 1 : (B1'=addBlock(B1,b1))&(setMiner1'=removeBlock(setMiner1,b1))&(Miner1'=Pow); 

[] (Miner1=5)&!(canBeInserted(B1,b1)) -> 1 : (Miner1'=-1);
endmodule


